{
    "name": "nice-yu/encryption",
    "description": "PHP Symmetric Encryptor, capable of generating JSON Web Tokens (JWT), or encoding and decoding, encrypting and decrypting passwords",
    "keywords": [
        "jwt-auth",
        "auth",
        "token",
        "password",
        "generate token",
        "verify token",
        "refresh token",
        "encrypt",
        "decrypt"
    ],
    "homepage": "https://github.com/nice-yu/encryption",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "nice-yu",
            "email": "nice-yu@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-openssl": "*"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4",
        "phpunit/phpunit": "9.6.*"
    },
    "autoload": {
        "files": [ "./src/Resources/functions.php" ],
        "psr-4": {
            "NiceYu\\Encryption\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [ "./src/Resources/functions.php" ],
        "psr-4": {
            "NiceYu\\Tests\\": "tests/"
        }
    }
}
